{
  "name": "autocomplaint-extension",
  "version": "5.0.0",
  "description": "AutoComplaint Chrome Extension with custom-trained ML classifier for order extraction",
  "main": "dist/amazon.bundle.js",
  "scripts": {
    "build": "webpack",
    "train": "node classifier_trainer.js",
    "test-classifier": "node -e \"import('./classifier_trainer.js').then(m => new m.ClassifierTrainer().evaluateModel())\"",
    "dev": "webpack --mode development --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@xenova/transformers": "^2.17.2",
    "core-js": "^3.45.0",
    "html2canvas": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babel-loader": "^10.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "keywords": [
    "chrome-extension",
    "machine-learning",
    "order-extraction",
    "tensorflow",
    "custom-classifier",
    "web-scraping"
  ]
}
