{
  "name": "autocomplaint-training-suite",
  "version": "1.0.0",
  "description": "Training suite for AutoComplaint ML models - separate from main extension",
  "main": "src/trainer.js",
  "type": "module",
  "scripts": {
    "install-deps": "npm install",
    "train": "node src/trainer.js",
    "train-custom": "node src/trainer.js --config custom-config.json",
    "evaluate": "node src/evaluator.js",
    "serve-model": "node src/model-server.js",
    "test-model": "node src/test-model.js",
    "export-model": "node src/export-model.js",
    "dev": "nodemon src/trainer.js",
    "web-ui": "node src/web-server.js",
    "build-for-extension": "node src/build-for-extension.js"
  },
  "keywords": [
    "machine-learning",
    "tensorflow",
    "text-classification",
    "order-detection",
    "training-suite"
  ],
  "author": "AutoComplaint Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "fs-extra": "^11.1.1",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "progress": "^2.0.3",
    "inquirer": "^9.2.7",
    "compromise": "^14.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
